            "fd_name": "3N5",					//设备名称
            "equivalenthour": "4.30",			//等效小时
            "pr": 84.56,						//PR值
            "fd_power_curr_unit": null,
            "fd_power_unit": "万kWh",			//发电量单位
            "id": 0,
            "fd_dev_id": "GS_S3_3N5",			//设备ID：父节点ID
            "fd_power": 147.64,					//发电量
            "fd_power_curr": 0,
            "fd_pw_curr": 0,					交流功率
            "fd_pdc_curr": 0,					直流功率
            "fd_radia_curr": 18.315,			//辐射量
            "fd_pr1": 0,		 				
            "fd_pr2": 0,
            "fd_log_date": null,				//时间
            "update_time": null
			fd_pw_curr_avg		平均交流功率
			fd_pdc_curr_avg		平均直流功率
			fd_radia_real_avg	平均瞬时辐照
			fd_radia_real		瞬时辐照


