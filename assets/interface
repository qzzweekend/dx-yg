						"sort": "1",		1:降序；2:升序
						"starttime":	"20170101000000",	开始时间:yyyyMMddHHmmss
						"endtime":		"20170104000000",	结束时间:yyyyMMddHHmmss
						"topn": "7",		取多少个
						"stationid": ""		电站ID：为空就是所有
						equivalenthour:		等效小时 



